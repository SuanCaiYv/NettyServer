<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nettyserver.org.dao.ServerFileMapper">
  <resultMap id="BaseResultMap" type="com.nettyserver.org.pojo.ServerFile">
    <id column="file_uuid" jdbcType="VARCHAR" property="fileUuid" />
    <result column="file_name" jdbcType="VARCHAR" property="fileName" />
    <result column="file_path" jdbcType="VARCHAR" property="filePath" />
    <result column="create_time" jdbcType="BIGINT" property="createTime" />
    <result column="master_email" jdbcType="VARCHAR" property="masterEmail" />
    <result column="version" jdbcType="INTEGER" property="version" />
  </resultMap>
  <select id="selectByFileUuid" parameterType="java.lang.String" resultMap="BaseResultMap">
    select *
    from server_file
    where file_uuid = #{fileUuid,jdbcType=VARCHAR}
  </select>
  <delete id="deleteByFileUuid" parameterType="java.lang.String">
    delete from server_file
    where file_uuid = #{fileUuid,jdbcType=VARCHAR}
  </delete>
  <insert id="insertServerFile" parameterType="com.nettyserver.org.pojo.ServerFile">
    insert into server_file (file_uuid, file_name, file_path, 
      create_time, master_email, version
      )
    values (#{fileUuid,jdbcType=VARCHAR}, #{fileName,jdbcType=VARCHAR}, #{filePath,jdbcType=VARCHAR}, 
      #{createTime,jdbcType=BIGINT}, #{masterEmail,jdbcType=VARCHAR}, #{version,jdbcType=INTEGER}
      )
  </insert>
  <update id="updateByFileUuid" parameterType="com.nettyserver.org.pojo.ServerFile">
    update server_file
    <set>
      <if test="fileName != null">
        file_name = #{fileName,jdbcType=VARCHAR},
      </if>
      <if test="filePath != null">
        file_path = #{filePath,jdbcType=VARCHAR},
      </if>
      <if test="createTime != null">
        create_time = #{createTime,jdbcType=BIGINT},
      </if>
      <if test="masterEmail != null">
        master_email = #{masterEmail,jdbcType=VARCHAR},
      </if>
      <if test="version != null">
        version = #{version,jdbcType=INTEGER},
      </if>
    </set>
    where file_uuid = #{fileUuid,jdbcType=VARCHAR}
  </update>
  <resultMap id="ServerFileMap" type="com.nettyserver.org.pojo.ServerFile">
    <id column="file_uuid" jdbcType="VARCHAR" property="fileUuid" />
    <result column="file_name" jdbcType="VARCHAR" property="fileName" />
    <result column="file_path" jdbcType="VARCHAR" property="filePath" />
    <result column="create_time" jdbcType="BIGINT" property="createTime" />
    <result column="master_email" jdbcType="VARCHAR" property="masterEmail" />
    <result column="version" jdbcType="INTEGER" property="version" />
  </resultMap>
  <select id="selectByFileNameAndEmail" resultMap="ServerFileMap">
    select * from server_file where file_name = #{param1, jdbcType=VARCHAR} and master_email = #{param2, jdbcType=VARCHAR}
  </select>
</mapper>